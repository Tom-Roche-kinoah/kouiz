{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    //"extends": "eslint:recommended",
    "rules": {
      "indent": ["error", 4],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "no-multi-spaces": "error",
      "no-trailing-spaces": "error"
    }
}